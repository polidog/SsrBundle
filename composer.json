{
    "name": "polidog/ssr-bundle",
    "description": "A JavaScript server side rendering for Symfony",
    "authors": [
        {
            "name": "polidog",
            "email": "polidogs@gmail.com"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",
        "koriym/baracoa": "1.x-dev",
        "symfony/framework-bundle": "~2.7|~3.0",
        "sensio/framework-extra-bundle": "~2.7|~3.0",
        "sabre/cache": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "phpmd/phpmd": "^2.6",
        "friendsofphp/php-cs-fixer": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Polidog\\SsrBundle\\": ""
        }
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit"
        ],
        "cs-fix": [
            "./vendor/bin/php-cs-fixer fix"
        ]
    }
}
